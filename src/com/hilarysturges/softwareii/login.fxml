<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="300.0" prefWidth="200.0" style="-fx-background-color: A4C3BA; -fx-border-color: 000000; -fx-border-width: 1; -fx-border-style: solid;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.hilarysturges.softwareii.LoginController">
   <children>
      <VBox layoutX="13.0" layoutY="98.0" prefHeight="192.0" prefWidth="176.0" spacing="10.0">
         <children>
            <Label fx:id="usernameLabel" text="Username:" />
            <TextField fx:id="username" />
            <Label fx:id="passwordLabel" text="Password:" />
            <TextField fx:id="password" />
            <Button fx:id="loginButton" mnemonicParsing="false" onAction="#login" text="Log In" />
         </children>
      </VBox>
      <Label fx:id="pleaseLoginLabel" layoutX="67.0" layoutY="30.0" text="Please log in" />
   </children>
</AnchorPane>
